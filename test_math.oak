BEGIN PROJ "math_test.project"
    BEGIN SECTION "math_demo"
        var x := 16
        var angle := 0.5
        
        print "Math Functions Demo"
        print "=================="
        
        print "x = " + x
        print "sqrt(x) = " + sqrt(x)
        print "abs(-5) = " + abs(-5)
        print "sin(0.5) = " + sin(angle)
        print "cos(0.5) = " + cos(angle)
        print "tan(0.5) = " + tan(angle)
        print "log(10) = " + log(10)
        print "exp(1) = " + exp(1)
        
        print "Math Constants"
        print "=============="
        print "PI = " + PI
        print "E = " + E
        
        ret "Math functions working!"
    END SECTION "math_demo"
END PROJ "math_test.project" 